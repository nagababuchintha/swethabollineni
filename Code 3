    <script>
        $(document).ready(function () {
            //tabify content
            $("#search").val("");
            $("#tabs").tabs();
            $("#print-link").button({icons: { secondary: "ui-icon-print" }}).click(function () { window.print(); });
            $("#disabled-link").click(function (event) {
                event.preventDefault();
                if ($(this).attr('a') !== undefined)
                    $(this).attr('a').removeAttr('href')
            });
            $("#map-link").click(function (event) {event.preventDefault();window.open('https://ags3.scgov.net/scpa/?esearch=0046050008&slayer=0');});
            $("#map-link,#disabled-link").button({icons: {secondary: "ui-icon-extlink"}});
            $("#tax-link").click(function (event) {
                event.preventDefault();
                var url = this.href;
                $("#tax-link-message").dialog(
                    {
                        resizable: false,
                        height: 250,
                        width: 500,
                        modal: true,
                        buttons:
                        {
                            "Yes, I want to leave": function () {
                                $(this).dialog("close");
                                //window.open('');
                                window.open(url);
                            },
                            "No": function () {
                                $(this).dialog("close");
                            }
                        }
                    });
            });
            $("#trim-link,#disabled-link").button({ icons: { secondary: "ui-icon-arrowthickstop-1-s" } });
            $("#tax-link,#disabled-link").button({ icons: { secondary: "ui-icon-extlink" } });
            $("#prc-link,#disabled-link").button({icons: {secondary: "ui-icon-arrowthickstop-1-s"}});
            $("#button-set-1").buttonset();
            $("#button-set-2").buttonset();
            $("#Buildings a").click(function (event) {
                event.preventDefault();
                $.ajax({
                    url: this.href,
                    type: "get",
                    cache: false,
                    success: function (results) {
                        $('#building-container').html(results);
                        $("#building-container").dialog(
                            {
                                resizable: true,
                                height: 810,
                                width: 600,
                                modal: true,
                                buttons:
                                {
                                    "Close": function () {
                                        $(this).dialog("close");
                                    }
                                }
                            });
                    }
                });
            });
        });
        function showInstrument(i) {
            var bp = i.split("/");
            if (bp.length > 1) {
                showBookPage(bp[0], bp[1]);
                return;
            }
            $('#insFrom').val(i);
            $('#insTo').val(i);
            $('#frminstrument').submit();
        }
        function showBookPage(b, p) {
            $('#insBook').val(b);
            $('#insPage').val(p);
            $('#frminstrument').submit();
        }

        // "horizontal vertical" alignment
        $("#prevstrap").tooltip({
            position: {
                my: "center center",
                at: "left-160 center-22",
                of: "#targetElemForTooltips",
				collision: "flipfit"
            }
        });
        $("#nextstrap").tooltip({
            position: {
                my: "center center",
                at: "center+160 center-22",
                of: "#targetElemForTooltips",
				collision: "flipfit"
            }
        });
    </script>


